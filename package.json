{
  "name": "sort-string",
  "displayName": "Sort String By Columns",
  "description": "",
  "version": "0.1.4",
  "publisher": "YojhanAmachuy",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "icon": "ico-sort-string.png",
  "contributes": {
    "keybindings": [
    {
        "key": "alt+ctrl+u",
        "command": "extension.sortByColumn7And8",
        "mac": "cmd+ctrl+u"
    }
    ],
    "commands": [
    {
      "command": "extension.sortByColumn7And8",
      "title": "Sort by Column 7 and 8"
    }
    ],
    "configuration": {
      "type": "object",
      "title": "Configuraciones de Mi Extensi√≥n",
      "properties": {
        "sortString.firstColumn": {
          "type": "integer",
          "default": 7,
          "description": "Primer columna a ordenar"
        },
        "sortString.secondColumn": {
          "type": "integer",
          "default": 8,
          "description": "Segunda columna a ordenar"
        },
        "sortString.splitString": {
          "type": "string",
          "default": ";",
          "description": "Separador de columnas"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.58.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YojhanYac/sort-shipments-status.git"
  }
}
